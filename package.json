    "prisma": "5.15.0",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2",
    "unplugin-swc": "1.4.3",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "1.6.0"
